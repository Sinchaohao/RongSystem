<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.iot.mapper.RainSecMapper">
    <resultMap type="RainSecurity" id="RainSecurityResult">
        <id property="rain" column="rain" />
        <result property="time" column="time"/>
        <result property="content" column="content"/>
    </resultMap>

    <select id="selectRainSecList" resultMap="RainSecurityResult">
        select
          rain,
          time
        from
          env_data
        where rain>10
        order by time ASC
    </select>

</mapper>
  <sql id="selectWaterpannelVo">
        select
        id,aid,tem,ph,N_data,P_data,Q_data,S_data,Fu_data,Bac_data,sampling_by,
        sampling_time,remark from water_data
    </sql>

    <select id="selectWaterpannelList" resultMap="WaterpannelResult">
        select
        id,aid,tem,ph,N_data,P_data,Q_data,S_data,Fu_data,Bac_data,sampling_by,
        sampling_time,remark from water_data
       order by id

    </select>

      <select id="selectListById" parameterType="Integer" resultType="map">

     select
        id,aid,tem,ph,N_data,P_data,Q_data,S_data,Fu_data,Bac_data,sampling_by,
        sampling_time,remark
        from iotdata.water_data
         where id=#{id}
    </select>

    <select id="selectListByAId" parameterType="String" resultType="map">

     select
        id,aid,tem,ph,N_data,P_data,Q_data,S_data,Fu_data,Bac_data,sampling_by,
        sampling_time,remark
        from water_data
         where aid=#{aid}
    </select>

